(defn parseFactorR (sum expr)
  (if (gt (count expr) 0)
      ((lambda (c)
        (if (and
            (gte c '0')
            (lte c '9'))
          (parseFactorR (add (mul 10 sum) (sub c '0')) (substr expr 1))
          (list sum expr)
        )
      ) (at expr 0))
      (list sum expr)
  )
)

(defn parseFactor (e)
  (parseFactorR 0 e)
)

(defn evalTerm (expr)
  (evalTermR (parseFactor expr))
)

(defn evalTermR (pair)
  ((lambda (lhs expr)
    (if (lte (count expr) 0)
      pair
      ((lambda (lookahead expr)
        (if (or (equals lookahead '*')
                (equals lookahead '/'))
          ((lambda (pair)
            ((lambda (rhs expr)
              (if (equals lookahead '*')
                (evalTermR (list (mul lhs rhs) expr))
                (evalTermR (list (div lhs rhs) expr))
              )
            ) (at pair 0) (at pair 1))
          ) (parseFactor expr))
          pair
        )
      ) (at expr 0) (substr expr 1))
    )
  ) (at pair 0) (at pair 1))
)

(defn evalExpr (e)
  (evalExprR (evalTerm e))
)

(defn evalExprR (pair)
  ((lambda (lhs expr)
    (if (lte (count expr) 0)
      pair
      ((lambda (lookahead expr)
        (if (or (equals lookahead '+')
                (equals lookahead '-'))
          ((lambda (pair)
            ((lambda (rhs expr)
              (if (equals lookahead '+')
                (evalExprR (list (add lhs rhs) expr))
                (evalExprR (list (sub lhs rhs) expr))
              )
            ) (at pair 0) (at pair 1))
          ) (evalTerm expr))
          pair
        )
      ) (at expr 0) (substr expr 1))
    )
  ) (at pair 0) (at pair 1))
)

(defn eval (e) (at (evalExpr e) 0))

(print (eval "22"))
(print (eval "22+22"))
(print (eval "22-11"))
(print (eval "10+51-17"))
(print (eval "3*5+10"))
(print (eval "10+3*5+10"))
