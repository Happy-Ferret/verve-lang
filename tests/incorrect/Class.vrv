// RUN: %not %verve | %check

class S {
  let x : Int
}

S() // CHECK-L: Missing properties: x
S(x: 0, y: 10) // CHECK-L: Unknown property: y

let s = S(x: 10)
"".y // CHECK-L: Not an object: Trying to access field y of value of type String
s.y // CHECK-L: Trying to access unknown property y of object of type S

s.x() // CHECK-L: Invalid application: applied to too many arguments
s.y() // CHECK-L: Unknown variable: y

"".y() // CHECK-L: Unknown variable: y
