// RUN: %not %verve | %check

class S {
  let x : Int
}

S {} // CHECK-L: Missing properties: x
S { x = 0, y = 10 } // CHECK-L: Unknown property: y

let s = S { x = 10 }
s.y // CHECK-L: Trying to access unknown property y of object of type S

s.x() // CHECK-L: Trying to access unknown method x in object of type S
s.y() // CHECK-L: Trying to access unknown method y in object of type S

"".y() // CHECK-L: Trying to call method y of non-object of type String
