// RUN: %verve | %check
class Square {
  let side : Int

  fn area() -> Int {
    int_mul(this.side, this.side)
  }
}

let x = Square { side = 10 }
x.side // CHECK-L: 10 : Int
x.area() // CHECK-L: 100 : Int


interface Shape<T> {
  fn area(T) -> Int
}

implementation Shape<Square> {
  fn area(x : Square) -> Int {
    int_mul(x.side, x.side)
  }
}

area(x) // CHECK-L: 100 : Int
